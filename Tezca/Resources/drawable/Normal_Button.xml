<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:state_pressed="true" >
    <shape android:shape="rectangle"  >
      <corners android:radius="3dip" />
      <stroke android:width="1dip" android:color="#ffffe0b2" />
      <gradient android:angle="-90" android:startColor="#ffffe0b2" android:endColor="#ffffe0b2"  />
    </shape>
  </item>
  <item android:state_focused="true">
    <shape android:shape="rectangle"  >
      <corners android:radius="3dip" />
      <stroke android:width="1dip" android:color="#ffffe0b2" />
      <solid android:color="#ffffe0b2"/>
    </shape>
  </item>
  <item >
    <shape android:shape="rectangle"  >
      <corners android:radius="3dip" />
      <stroke android:width="1dip" android:color="#ffff8f00" />
      <gradient android:angle="-90" android:startColor="#ffff8f00" android:endColor="#ffff8f00" />
    </shape>
  </item>
</selector>